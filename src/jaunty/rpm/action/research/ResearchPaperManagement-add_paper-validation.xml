<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC
  		"-//OpenSymphony Group//XWork Validator 1.0//EN" 
  		"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="paperName">
		<field-validator type="requiredstring">
			<message>请填写论文名称</message>
		</field-validator>
	</field>
	<field name="paperAuthor">
		<field-validator type="requiredstring">
			<message>请填写作者</message>
		</field-validator>
	</field>
	<field name="paperAttribute">
		<field-validator type="required">
			<message>请填写论文类别</message>
		</field-validator>
	</field>
	<field name="note">
		<field-validator type="requiredstring">
			<message>请填写备注</message>
		</field-validator>
	</field>
</validators>
